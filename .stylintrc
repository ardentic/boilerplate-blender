{
  "blocks": {
    "expect": "always",
    "error": true
  },
  "brackets": {
    "expect": "always",
    "error": true
  },
  "colons": {
    "expect": "always",
    "error": true
  },
  "commaSpace": {
    "expect": "always",
    "error": true
  },
  "commentSpace": {
    "expect": "always",
    "error": true
  },
  "cssLiteral": {
    "expect": "never",
    "error": true
  },
  "duplicates": {
    "expect": true,
    "error": true
  },
  "efficient": {
    "expect": "always",
    "error": true
  },
  "extendPref": {
    "expect": "@extends",
    "error": true
  },
  "indentPref": {
    "expect": 2,
    "error": true
  },
  "leadingZero": {
    "expect": "always",
    "error": true
  },
  "namingConvention": {
    "expect": "camelCase",
    "error": true
  },
  "none": {
    "expect": "always",
    "error": true
  },
  "noImportant": {
    "expect": true,
    "error": true
  },
  "parenSpace": {
    "expect": false,
    "error": true
  },
  "placeholders": {
    "expect": "always",
    "error": true
  },
  "prefixVarsWithDollar": {
    "expect": "always",
    "error": true
  },
  "quotePref": {
    "expect": "single",
    "error": false
  },
  "semicolons": {
    "expect": "always",
    "error": true
  },
  "stackedProperties": {
    "expect": "never",
    "error": true
  },
  "trailingWhitespace": {
    "expect": "never",
    "error": true
  },
  "valid": {
    "expect": true,
    "error": true
  },
  "zeroUnits": {
    "expect": "never",
    "error": true
  },

  "mixed": false,
  "colors": false,
  "universal": false,
  "sortOrder": false,
  "maxErrors": false,
  "globalDupe": false,
  "depthLimit": false,
  "maxWarnings": false,
  "zIndexNormalize": false,
  "namingConventionStrict": false
}
